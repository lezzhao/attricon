{
  "name": "@attricon/monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@9.1.1",
  "description": "_description_",
  "author": "lezzhao <zhaole.work@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/lezzhao/attricon#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lezzhao/attricon.git"
  },
  "bugs": "https://github.com/lezzhao/attricon/issues",
  "keywords": [],
  "sideEffects": false,
  "scripts": {
    "build": "pnpm -r --filter=./packages/* run build",
    "clean": "rimraf packages/*/dist",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "taze": "taze minor -wIr && pnpm -r --parallel run update-post",
    "play": "vite playground"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.0",
    "@attricon/core": "workspace:*",
    "@attricon/vite": "workspace:*",
    "@iconify/types": "^2.0.0",
    "@types/node": "^20.12.12",
    "bumpp": "^9.4.1",
    "eslint": "^9.2.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.1.1",
    "rimraf": "^5.0.7",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.8",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^5.2.11",
    "vitest": "^1.6.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
